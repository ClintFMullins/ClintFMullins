.intro
  .clint

    .prof-pic
      =image_tag '3e2ccec.jpeg', class: 'prof-img'
      .caption My name is Clint Mullins.
  .filters
    %input{type: 'submit', value: 'all', class: 'all'}
    %input{type: 'submit', value: 'skills', class: 'skills'}
    %input{type: 'submit', value: 'projects', class: 'projects'}
    %input{type: 'submit', value: 'positions', class: 'positions'}
    %input{type: 'submit', value: 'hobbies', class: 'hobbies'}
.all-experiences
  - @experiences.each_with_index do |experience, i|
    = render partial: 'experience', locals: {experience: experience, i: i }